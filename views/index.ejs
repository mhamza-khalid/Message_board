<html>
  <head>
    <title><%= title %></title>
  </head>
  <body>
    <% messages.forEach((message, index) => {%>
        <div><%= message.text %></div>
        <div><%= message.user %></div>
        <div>
            <%= message.added %>
            <a href="/message?position=<%= index %>"><button>View message</button></a>
        </div>
        <hr></hr>
    <% }); %>
    <a href="http://localhost:3000/new"> Add a new message</a>
  </body>
</html>